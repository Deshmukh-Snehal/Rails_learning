<p id="notice"><%= notice %></p>

<h1>Appoinmentdetails</h1>

<table>
  <thead>
    <tr>
      <th>Doctor Name</th>
      <th>Player Name</th>
      <th>Appoinment details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appoinmentdetails.each do |appoinmentdetail| %>
      <tr>
        <td><%= appoinmentdetail.doctor.name %></td>
        <td><%= appoinmentdetail.player.name %></td>
        <td><%= appoinmentdetail.appoinment_details %></td>
        <td><%= link_to 'Show', appoinmentdetail %></td>
        <td><%= link_to 'Edit', edit_appoinmentdetail_path(appoinmentdetail) %></td>
        <td><%= link_to 'Destroy', appoinmentdetail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appoinmentdetail', new_appoinmentdetail_path %>
