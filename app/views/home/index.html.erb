<p id="notice"><%= notice %></p>
<h1>Users</h1>
<table class="table table-bordered" id='user'>
  <thead class="table-dark">
    <tr>
      <th>email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><img src="<%= user.image %>"></td>
        <td><%= user.email%></td>
      </tr>
    <% end %>
  </tbody>
</table>