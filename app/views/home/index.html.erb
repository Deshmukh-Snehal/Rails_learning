<p id="notice"><%= notice %></p>
<div class="container">
  <h1>Users</h1>
  <table class="table table-bordered" id='userTable'>
    <thead class="table-dark">
      <tr>
        <th>Image</th>
        <th>First Name</th>
        <th>Last Name</th>
      </tr>
    </thead>
    <tbody>
    <% User.all.each do |user| %>
      <tr>
        <td><img src="<%= user.image %>"></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>